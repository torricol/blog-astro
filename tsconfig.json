{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@scripts/*": [
        "./src/scripts/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ]
    }
  }
}